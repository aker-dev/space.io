<div class="topbar container-fluid navbar-fixed-top">
    <div class="row">
        <div class="col-md-4" data-pg-collapsed>
            <a class="navbar-brand" href="#">(S)PACE CROUS</a>                      
        </div>
        <div class="col-md-8 text-right">
            <a class="btn btn-tag btn-default btn-lg tag-id-all icon-home" href="#">All</a>

            <%
            for(var tag in tags){ %>
                <a class="btn btn-tag btn-lg btn-<%= tags[tag].color %> tag-id-<%= tag %> <%= tags[tag].icon %>" data-tag-id="<%= tag %>" href="#"> <%= tags[tag].title %> </a>
            <% };
            %>         

        </div>
    </div>
</div>